<messageML>
    <div class="entity" data-entity-id="githubPublic">
        <card class="barStyle" iconSrc="${entity['githubPublic'].iconURL}">
            <header>
                <span class="tempo-text-color--normal">Repository</span>
                <a class="tempo-text-color--link" href="${entity['githubPublic'].repository.url}">${entity['githubPublic'].repository.fullName}</a>
                <span class="tempo-text-color--normal">was made</span>
                <span class="tempo-text-color--green"><b>public</b></span>
                <span class="tempo-text-color--normal">by</span>
                <span class="tempo-text-color--normal"><b>${entity['githubPublic'].sender.name}</b></span>
            </header>
        </card>
    </div>
</messageML>
